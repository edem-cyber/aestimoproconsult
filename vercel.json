{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.php"
        },
        {
            "src": "/team",
            "dest": "/api/demo-consulting-company.php"
        },
        {
            "src": "/contact",
            "dest": "/api/demo-consulting-contact.php"
        },
        {
            "src": "/approach",
            "dest": "/api/demo-consulting-process.php"
        },
        {
            "src": "/services",
            "dest": "/api/demo-consulting-services.php"
        },
        {
            "src": "/services-details",
            "dest": "/api/demo-consulting-services-details.php"
        },
        {
            "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|mp4|webm|pdf))",
            "dest": "/$1"
        }
    ]
}
